{
  "name": "@ideanick/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/.eslintcache .",
    "lint:fix": "pnpm lint --fix",
    "stylelint": "stylelint --cache --cache-location node_modules/.cache/stylelint/.stylelintcache \"src/**/*.scss\"",
    "stylelint:fix": "stylelint --cache --cache-location node_modules/.cache/stylelint/.stylelintcache \"src/**/*.scss\" --fix",
    "preview": "vite preview",
    "prettify": "prettier --cache --write \"src/**/*.{js,jsx,ts,tsx,json,yml,scss}\" \"!**/{node_modules,dist}/**/*\"",
    "types": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project tsconfig.node.json && tsc --noEmit --project tsconfig.app.json"
  },
  "dependencies": {
    "@ideanick/backend": "workspace:*",
    "@tanstack/react-query": "^5.79.0",
    "@trpc/client": "^11.1.4",
    "@trpc/tanstack-react-query": "^11.1.4",
    "include-media": "^2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.1",
    "sass": "^1.89.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.28",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^3.5.3",
    "stylelint": "^16.20.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
